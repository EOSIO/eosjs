{
  "name": "@blockone/b1x-eosjs",
  "version": "22.0.0",
  "description": "Talk to eos API",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "lint": "eslint src/",
    "lint-fix": "eslint -c tslint.json -p tsconfig.json --fix",
    "test": "jest src/",
    "build": "NODE_ENV=production tsc -p ./tsconfig.json",
    "clean": "rm -rf dist",
    "docs-init": "sh .docs/scripts/init.sh",
    "docs-build": "sh .docs/scripts/build.sh",
    "docs-serve": "python -m SimpleHTTPServer",
    "docs-publish": "sh .docs/scripts/publish.sh",
    "format": "prettier src/ --write"
  },
  "author": "block.one",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EOSIO/eosjs.git"
  },
  "dependencies": {
    "elliptic": "^6.5.4",
    "hash.js": "1.1.7",
    "pako": "2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@types/elliptic": "6.4.12",
    "@types/jest": "^26.0.20",
    "@types/pako": "^1.0.1",
    "babel-jest": "^26.6.3",
    "eosjs-ecc": "^4.0.7",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^26.5.2",
    "typescript": "4.2.2"
  }
}
